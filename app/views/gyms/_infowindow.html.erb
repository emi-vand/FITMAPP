<div class="map-card">
  <h5 class="map-card-name" style="color: #E4F2F6"> <%= link_to gym.name, gym_path(gym) %> </h5>
  <p class="map-card-address"> <%= gym.address %> </p>
  <%= link_to "Directions", "https://www.google.com/maps/dir/?api=1&travelmode=walking&layer=traffic&destination=#{gym.latitude},#{gym.longitude}", class: "btn-small btn-primary p-2 rounded map-card-btn", target: :_blank %>
</div>


<!DOCTYPE html>
<html>
  <head>
    <title>FITMAPP - Healthy made simple</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <meta name="description" content="<%= "Find the best healthy restaurants and gyms near you, wherever you are!" %>" />

    <!-- Facebook Open Graph data -->
    <meta property="og:title" content="<%= "FITMAPP - Healthy Made Simple" %>" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="<%= "www.fitmapp.co.uk" %>" />
    <meta property="og:image" content="<%= "meta-tag.jpg" %>" />
    <meta property="og:description" content="<%= "Find the best healthy restaurants and gyms near you, wherever you are!" %>" />
    <meta property="og:site_name" content="<%= "FITMAPP" %>" />

    <!-- Twitter Card data -->
    <meta name="twitter:card" content="summary_large_image"/>
    <meta name="twitter:site" content="<%= "fitmapp" %>"/>
    <meta name="twitter:title" content="<%= "FITMAPP - Healthy Made Simple" %>"/>
    <meta name="twitter:description" content="<%= "Find the best healthy restaurants and gyms near you, wherever you are!" %>"/>
    <meta name="twitter:creator" content="<%= "fitmapp" %>"/>
    <meta name="twitter:image:src" content="<%= "meta-tag.jpg" %>"/>

    <%= favicon_link_tag "fav_marker_copy.png"%>

    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="mobile-web-app-capable"  content="yes">

    <link rel="manifest" href="<%= asset_path 'manifest.json' %>">

    <link href="data:image/x-icon;base64," rel="icon" type="image/x-icon" />    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <script src='https://api.mapbox.com/mapbox-gl-js/v1.12.0/mapbox-gl.js'></script>
    <link href='https://api.mapbox.com/mapbox-gl-js/v1.12.0/mapbox-gl.css' rel='stylesheet' />
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload', defer: true %>
  </head>

  <body>
    <%= render 'shared/navbar' %>
    <%= render 'shared/flashes' %>

    <%= yield %>
    <%= render 'shared/nav_bottom' %>
    <%= render 'shared/footer' %>
  </body>
</html>
